CREATE DATABASE arsenal_db DEFAULT CHARSET=utf8;;

CREATE TABLE `Вопросы` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `ФИО` text,
  `Email` text,
  `Телефон` text,
  `Вопрос` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8;

CREATE TABLE `Отзывы` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `ФИО` text,
  `Отзыв` text,
  `Фотография` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

CREATE TABLE `Заказы` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `ФИО` text,
  `Email` text,
  `Телефон` text,
  `Доставка` text,
  `Город` text,
  `Адрес` text,
  `Заказ` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8;

CREATE TABLE `Изоляционные_материалы` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `Наименование` text,
  `Цена/шт` double DEFAULT NULL,
  `Количество` int(11) DEFAULT NULL,
  `Фото` text,
  `Описание` text,
  `Характеристики` text,
  `Тип` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

CREATE TABLE `Кровля` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `Наименование` text,
  `Цена/шт` double DEFAULT NULL,
  `Количество` int(11) DEFAULT NULL,
  `Фото` text,
  `Описание` text,
  `Характеристики` text,
  `Тип` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

CREATE TABLE `Изоляционные_материалы` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `Наименование` text,
  `Цена/шт` double DEFAULT NULL,
  `Количество` int(11) DEFAULT NULL,
  `Фото` text,
  `Описание` text,
  `Характеристики` text,
  `Тип` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

INSERT INTO `Изоляционные_материалы` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(1, 'Экструдированный пенополистирол XPS Укна 50 мм Т-15 585x1185 мм 0.69 м', 292, 318, 'img/catalog/insulation_materials/1.jpeg', 'Утеплитель XPS — теплоизоляционная плита, обеспечивающая также поглощение воздушных и конструкционных шумов. Производится методом экструзии (XPS — экструдированный пенополистирол), имеет равномерную пористую структуру с закрытыми ячейками.', 'Площадь покрытия продуктом (м²):\r\n0.69<br>\r\nТолщина (мм):\r\n50<br>\r\nПлотность (кг/м³):\r\n22<br>\r\nКоэффициент теплопроводности (Вт/м°С):\r\n0.035<br>\r\nМесто использования:\r\nВнутренний / наружный<br>\r\nНазначение:\r\nФундамент, Каркасная стена, Стяжка пола, Скатная крыша<br>\r\nГруппа горючести:\r\nГ4 (сильногорючие)<br>\r\nВлагостойкость:\r\nДа<br>\r\nКоличество матов в упаковке (шт):\r\n8<br>\r\nДлина (м):\r\n1.185<br>\r\nШирина (м):\r\n0.585<br>\r\nВес нетто (кг):\r\n0.664<br>\r\nТип продукта:\r\nXPS<br>\r\nМарка:\r\nУКНА<br>\r\nСтрана производства:\r\nРоссия', 'Изоляционные_материалы');
INSERT INTO `Изоляционные_материалы` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(2, 'Утеплитель ТеплоKnauf Стена плита 50 мм 6 м²', 1150, 23, 'img/catalog/insulation_materials/2.jpeg', 'Минеральный утеплитель ТеплоKNAUF Стена Extra разработан специально для теплоизоляции наружных каркасных стен.<br>\r\nИзготовлен из натуральных природных компонентов, в его составе не содержится фенол-формальдегидных и акриловых смол, это безопасный материал. Он не колется и не вызывает зуда при монтаже, мало пылит и мягкий на ощупь, без неприятных запахов. В том числе поэтому утеплитель ТеплоKNAUF Стена Extra рекомендуется применять для детских комнат или спален.', 'Площадь покрытия продуктом (м²): 6<br>\r\nТолщина (мм):\r\n50<br>\r\nПлотность (кг/м³):\r\n22<br>\r\nГруппа горючести:\r\nНегорючийv\r\nКоэффициент теплопроводности (Вт/м°С):\r\n0.034<br>\r\nМесто использования:\r\nВнутренний<br>\r\nНазначение:\r\nКаркасная стена, Стяжка пола<br>\r\nДлина (м):\r\n1<br>\r\nШирина (м):\r\n0.6<br>\r\nОбъем (м³):\r\n0.3<br>\r\nВес, кг:\r\n8.46<br>\r\nОсновной материал:\r\nСтекловатаv\r\nТип продукта:\r\nМинеральная вата<br>\r\nКоличество матов в упаковке (шт):\r\n10<br>\r\nСтрана производства:\r\nРоссия<br>\r\nМарка:\r\nKNAUF', 'Изоляционные_материалы');
INSERT INTO `Изоляционные_материалы` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(3, 'Звукоизоляция АкустиКнауф плита 50 мм 610x1000 мм 6 м²', 925, 0, 'img/catalog/insulation_materials/3.jpeg', 'Звукоизоляция АкустиKnauf используется для уменьшения или устранения акустического дискомфорта в жилых или общественных помещениях. Применяется для отделки стен, потолков, перегородок и других строительных конструкций.<br>Материал изготовлен на основе минеральной ваты, характеризуется высокими показателями жесткости и упругости. Легко режется ножом, что упрощает монтажные работы. Поставляется в рулонах, упакованных в герметичную полиэтиленовую пленку. Площадь покрытия — 6 м². Толщина мата 50 мм.', 'Площадь покрытия продуктом (м²):\r\n6<br>\r\nТолщина (мм):\r\n50<br>\r\nПлотность (кг/м³):\r\n15<br>\r\nГруппа горючести:\r\nНегорючий<br>\r\nКоэффициент теплопроводности (Вт/м°С):\r\n0.037<br>\r\nСнижение акустического воздушного шума (в дБ):\r\n57<br>\r\nМесто использования:\r\nВнутренний<br>\r\nНазначение:\r\nКаркасная стена, Перегородка, Потолок<br>\r\nДлина (м):\r\n1<br>\r\nШирина (м):\r\n0.61<br>\r\nОбъем (м³):\r\n0.305<br>\r\nВес, кг:\r\n4.28<br>\r\nОсновной материал:\r\nСтекловата<br>\r\nТип продукта:\r\nМинеральная вата<br>\r\nКоличество матов в упаковке (шт):\r\n10<br>\r\nСтрана производства:\r\nРоссия<br>\r\nМарка:\r\nKNAUF', 'Изоляционные_материалы');
INSERT INTO `Изоляционные_материалы` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(4, 'Звукоизоляция Евроблок 20 мм', 138, 2344, 'img/catalog/insulation_materials/4.jpeg', 'Евроблок – листовой материал для тепло- и звукоизоляции, содержащий несколько слоев вспененного полиэтилена различной плотности и толщины. Применяется для звукоизоляции под подвесной потолок. Снижение бытового шума до 10%. Евроблок не выделяет токсичных веществ, не впитывает влагу и совместим с любыми поверхностями.', 'Толщина (мм):\r\n20<br>\r\nШирина (м):\r\n0.6<br>\r\nДлина (м):\r\n1<br>\r\nКоличество в наборе:\r\n1<br>\r\nВес, кг:\r\n0.245<br>\r\nПлощадь покрытия продуктом (м²):\r\n0.6<br>\r\nОсновной материал:\r\nПолиэтилен<br>\r\nПлотность (кг/м³):\r\n30<br>\r\nСнижение акустического воздушного шума (в дБ):\r\n13<br>\r\nКоэффициент теплопроводности (Вт/м°С):\r\n0.04<br>\r\nМесто использования:\r\nВнутренний<br>\r\nНазначение:\r\nПотолок, Пол, Стена<br>\r\nСтрана производства:\r\nРоссия', 'Изоляционные_материалы');
INSERT INTO `Изоляционные_материалы` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(5, 'Праймер битумный Технониколь №01 16кг/20 л', 3534, 452, 'img/catalog/insulation_materials/5.jpeg', 'Праймер битумный «ТЕХНОНИКОЛЬ» №01 применяется для подготовки основания перед укладкой кровельного или гидроизоляционного материала, в том числе наплавляемого или самоклеющегося. Купив этот праймер, можно подготовить к укладке гидроизоляции шероховатые, пористые и пыльные основания.<br>Праймер следует наносить с помощью капроновой щетки или кисти, втирая его в поверхность. Необходимо соблюдать технику безопасности: работать вдали от огня, при хорошем проветривании, избегая попадания в глаза и на кожу.', 'Вес, кг:\r\n17.399<br>\r\nОбъем (л):\r\n20<br>\r\nРасход (л/м²):\r\n0,25-0,35<br>\r\nВремя высыхания (ч):\r\n12<br>\r\nОсновной материал:\r\nБитум<br>\r\nТип продукта:\r\nБитумный праймер<br>\r\nНазначение:\r\nФундамент, Кровля, Пол<br>\r\nМинимальная температура применения (°C):\r\n-20<br>\r\nМаксимальная температура применения (°C):\r\n40<br>\r\nМассовая доля нелетучих веществ (%):\r\n45-55<br>\r\nТип упаковки:\r\nБанка/Горшок<br>\r\nСтрана производства:\r\nРоссия', 'Изоляционные_материалы');

INSERT INTO `Кровля` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(1, 'Лист гладкий 0.35 мм 1250x2000 мм оцинкованный', 835, 115, '', 'Лист гладкий предназначен для строительства и хозяйственной деятельности. Используется также при герметизации и изготовлении отливов для труб отопления или вытяжек.\r\nЛист изготовлен из оцинкованной стали.\r\nДлина — 200 см, ширина — 125 см. Страна производства — Россия.\r\n\r\nОсобенности листа:\r\n\r\nЛегкий и прочный кровельный материал, устойчивый к механическим воздействиям.\r\nХорошо сгибается, сверлится или режется.\r\nПодходит для крыш любой конфигурации.\r\nПросто монтируется.\r\nУстойчив к воздействию ультрафиолета, атмосферных осадков и перепадам температур.\r\nНе теряет своих свойств в ходе эксплуатации.', 'Цвет:\r\nСерый<br>\r\nТип продукта:\r\nМеталлический лист<br>\r\nВнешний вид поверхности:\r\nГладкий<br>\r\nОсновной материал:\r\nСталь<br>\r\nДлина (см):\r\n200<br>\r\nШирина (см):\r\n125<br>\r\nТолщина (мм):\r\n0.35<br>\r\nРазмер (Д х Ш х В) (мм):\r\n2000х1250х0,35<br>\r\nВес, кг:\r\n6.5<br>\r\nПокрытие:\r\nОцинкованный<br>\r\nСтрана производства:\r\nРоссия', 'Кровля');
INSERT INTO `Кровля` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(2, 'Карнизная планка Артстройсити 10х60х68х2000мм 0.4мм RAL 8017', 461, 110, '', 'Карнизная планка — это уголок, который устанавливается на обрешетку вплотную к свесу. Одно плечо этого уголка накрывает крайние бруски обрешетки или первые 100-150 мм сплошного настила, второе направлено вниз и образует вместе с водосточным желобом узел, который закрывает боковую грань обрешетки и торцы контрреек.', 'Тип продукта:\r\nПланка карнизная<br>\r\nОсновной материал:\r\nПолиэстер<br>\r\nТолщина (мм):\r\n0.4<br>\r\nПокрытие:\r\nБез покрытия<br>\r\nЦвет:\r\nКоричневый<br>\r\nПрименение продукта:\r\nЗащищает нижнюю доску обрешётки и лобовую доску от воды<br>\r\nРазмер (Д х Ш х В) (мм):\r\n2000 х 200 х 0.4<br>\r\nВес, кг:\r\n1.5<br>\r\nМарка:\r\nАРТСТРОЙСИТИ<br>\r\nСтрана производства:\r\nРоссия<br>', 'Кровля');
INSERT INTO `Кровля` (`id`, `Наименование`, `Цена/шт`, `Количество`, `Фото`, `Описание`, `Характеристики`, `Тип`)
VALUES
	(3, 'Снегозадержатель для металлической кровли трубчатый 3 м RAL 8017 коричневый', 1488, 0, '', 'Снегозадержатель трубчатый длиной 3 м служит для предотвращения схода снега с кровли здания. Создает безопасные условия работы на крыше при ее ремонте или обслуживании, служит защитой водосточных систем. Снегозадержатель — незаменимый элемент кровли в регионах с обильными снегопадами. Крепится посредством кронштейнов саморезами с уплотнителями, совместим с разными кровельными покрытиями. Цвет конструкции — коричневый. Страна производства — Россия.', 'Тип продукта:\r\nСнегозадержатель <br>\r\nОсновной материал:\r\nСталь<br>\r\nПокрытие:\r\nОцинкованный<br>\r\nЦветовая палитра по RAL:\r\nRAL 8017<br>\r\nЦвет:\r\nКоричневый<br>\r\nРазмер (Д х Ш х В) (мм):\r\n3000х370х170<br>\r\nВес, кг:\r\n4.6<br>\r\nСтрана производства:\r\nРоссия<br>', 'Кровля');
